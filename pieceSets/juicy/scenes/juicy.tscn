code = "shader_type canvas_item;
void fragment(){
    vec2 position = UV;
    float pi = 3.14;
    float distance_to_center = distance(position, vec2(0.5, 0.5));
    float rotation_index = 5.0 * distance_to_center * pi * sin(TIME/20.0); // 6 is rotation speed
	float move_speed = 5.0;
	float move_distance = 0.5;
	
    // move back
    position += vec2(UV * 0.5 * sin(TIME));
	
	COLOR = texture(TEXTURE, position);
    
}"
